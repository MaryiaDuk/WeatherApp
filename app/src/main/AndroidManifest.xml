<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.weather.app.testapp">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:name="com.weather.app.testapp.app.TestAppApplication"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme">
        <activity android:name="com.weather.app.testapp.ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:screenOrientation="portrait"
            android:name="com.weather.app.testapp.ui.activity.ModelInfoActivity"></activity>

    </application>

</manifest>
